    "generator",
    "automation",
    "node-canvas"
  ],
  "author": "Artem Ryazanov",
  "license": "MIT",
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || ^24.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "cheerio": "^1.1.2",
    "commander": "^14.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "eslint": "^9.17.0",
    "globals": "^17.1.0",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "prettier": "^3.4.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.50.0"
  },
  "directories": {
    "test": "tests"
  }
