{
  "name": "esrb-slate-gen",
  "version": "1.0.9",
  "description": "CLI tool to generate ESRB rating slates",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artryazanov/esrb-slate-gen.git"
  },
  "bugs": {
    "url": "https://github.com/artryazanov/esrb-slate-gen/issues"
  },
  "homepage": "https://github.com/artryazanov/esrb-slate-gen#readme",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "bin": {
    "esrb-gen": "dist/index.js"
  },
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "dev": "ts-node src/index.ts",
    "download-assets": "ts-node scripts/download_assets.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
